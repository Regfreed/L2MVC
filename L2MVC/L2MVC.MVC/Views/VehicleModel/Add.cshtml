@model L2MVC.MVC.Models.AddVehicleModelViewModel
@{
}

<form method="post" action="Add">
    <div class="mb-3">
        <label for="" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Abrv</label>
        <input type="text" class="form-control" asp-for="Abrv">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Maker</label>
        <select class="form-select" asp-items="Model.MakerList" asp-for="MakeId"></select>
    </div>
  
    <button type="submit" class="btn btn-primary">Submit</button>
</form>